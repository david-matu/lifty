package com.dave.hobby.lift.model;

/**
 * 
 * @author David
 *	Dec 27, 2022
 */
public enum Status {
	IN_MOTION ("Lift is Travalling"),
	STOPPED ("Lift has stopped at Checkpoint"),
	FAILED ("Lift is in Failed State. Evacuation should be done by Rescue Team");
	
	private final String status;
	
	Status(String status) {
		this.status = status;
	}
}
