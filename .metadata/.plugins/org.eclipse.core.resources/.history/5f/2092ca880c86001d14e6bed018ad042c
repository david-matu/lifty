package com.dave.hobby.lift.model;

/**
 * 
 * @author David
 * Dec 27, 2022
 * 
 * Enlist all floors so that they are referenced from a single source
 * 
 */
public enum Floor {
	BP ("Basement Parking"),
	00 ("Ground Floor (Floor Zero)"),
	01 ("Office 1"),
	02 (""),
	3 (""),
	4 (""),
	5 (""),
	6 (""),
	7 (""),
	8 (""),
	9 (""),
	10 ("")
	
	private final String floor;
	
	Floor(String floor) {
		this.floor = floor;
	}

	public String getStatus() {
		return floor;
	}
}
