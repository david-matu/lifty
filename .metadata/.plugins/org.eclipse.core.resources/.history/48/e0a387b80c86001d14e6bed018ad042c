package com.dave.hobby.lift.model;

/**
 * 
 * @author David
 * Dec 27, 2022
 * 
 * Enlist all floors so that they are referenced from a single source
 * 
 */
public enum Floor {
	BP ("Basement Parking"),
	Zer0 ("Ground Floor (Floor Zero)"),
	1 ("Office 1"),
	2 ("Office 2"),
	3 ("Office 3"),
	4 ("Office 4"),
	5 ("Office 5"),
	6 ("Office 6"),
	7 ("Office 7"),
	8 ("Office 8"),
	9 ("Office 9"),
	10 ("Office 10")
	
	private final String floor;
	
	Floor(String floor) {
		this.floor = floor;
	}

	public String getStatus() {
		return floor;
	}
}
