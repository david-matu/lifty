package com.dave.hobby.lift.model;

/**
 * 
 * @author David
 * Dec 27, 2022
 * 
 * Enlist all floors so that they are referenced from a single source
 * 
 */
public enum Floor {
	BP ("Basement Parking"),
	GF ("Ground Floor (Floor Zero)"),
	L1 ("Office 1"),
	L2 ("Office 2"),
	L3 ("Office 3"),
	L4 ("Office 4"),
	L5 ("Office 5"),
	L6 ("Office 6"),
	L7 ("Office 7"),
	L8 ("Office 8"),
	L9 ("Office 9"),
	L10 ("Office 10")
	
	private final String floor;
	
	Floor(String floor) {
		this.floor = floor;
	}

	public String getStatus() {
		return floor;
	}
}
